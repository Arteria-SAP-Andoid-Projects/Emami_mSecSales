    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/padding_point_five_dip"
        android:background="@color/line_color"
        tools:ignore="SmallSp" />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/vertical_line_color"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/width_100_dip"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_point_one_dip"
            android:layout_marginRight="@dimen/margin_point_one_dip"
            android:layout_weight="1"
            android:background="@color/light_alice_blue"
            android:gravity="center"
            android:text="@string/lbl_kpi"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="@dimen/medium_text" />

        <TextView
            android:layout_width="@dimen/width_forty_dip"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_point_one_dip"
            android:layout_marginRight="@dimen/margin_point_one_dip"
            android:layout_weight="1"
            android:background="@color/light_alice_blue"
            android:gravity="center"
            android:text="@string/lbl_month_target"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="@dimen/medium_text" />

        <TextView
            android:layout_width="@dimen/width_forty_dip"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_point_one_dip"
            android:layout_marginRight="@dimen/margin_point_one_dip"
            android:layout_weight="1"
            android:background="@color/light_alice_blue"
            android:gravity="center"
            android:text="@string/lbl_mtda"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="@dimen/medium_text" />

        <TextView
            android:layout_width="@dimen/width_twenty"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_point_one_dip"
            android:layout_marginRight="@dimen/margin_point_one_dip"
            android:layout_weight="1"
            android:background="@color/light_alice_blue"
            android:gravity="center"
            android:text="@string/lbl_crr"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="@dimen/medium_text" />

        <TextView
            android:layout_width="@dimen/width_twenty"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_point_one_dip"
