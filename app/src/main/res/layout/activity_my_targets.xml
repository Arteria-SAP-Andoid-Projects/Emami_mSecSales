    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/HeaderTileBackground">

        <TextView
            android:id="@+id/tv_main_menu_header"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/title_my_targets"
            android:textColor="@color/header_tile_text_color"
            android:textSize="@dimen/large_text" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/padding_four_dip" >

        <TextView
            android:layout_width="@dimen/width_zero_dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="start|center_vertical"
            android:paddingLeft="@dimen/padding_three_dip"
            android:paddingStart="@dimen/padding_three_dip"
            android:paddingRight="@dimen/padding_zero_dip"
            android:paddingEnd="@dimen/padding_zero_dip"
            android:text="@string/lbl_month"
            android:textColor="@color/TEXT_COLOR"
            android:textSize="@dimen/medium_text" />

        <TextView
            android:layout_width="@dimen/width_zero_dip"
            android:layout_height="match_parent"
            android:layout_weight="0.1"
            android:gravity="start"
            android:text="@string/lbl_semi_colon"
            android:textColor="@color/BLACK"
            android:textSize="@dimen/medium_text"
            android:width="2dip" />

        <TextView
            android:id="@+id/tv_traget_month"
            android:layout_width="@dimen/width_zero_dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="start"
            android:paddingLeft="@dimen/padding_three_dip"
            android:paddingRight="@dimen/padding_three_dip"
            android:textColor="@color/TEXT_COLOR"
            android:textSize="@dimen/medium_text" />
    </LinearLayout>


    <ScrollView
        android:id="@+id/scroll_my_targets_header"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true">

    </ScrollView>

