<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WHITE"
    android:id="@+id/ll_so_create_line_item"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/width_zero_dip"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="2dp"
                android:paddingBottom="2dp"
                android:orientation="horizontal"
                android:gravity="center_vertical">

                <TextView
                    android:id="@+id/tv_item_so_review_ord_qty"
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text"/>
                <TextView
                    android:id="@+id/tv_item_so_review_primary_scheme"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="cc"
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/tv_item_so_review_sec_scheme"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/tv_item_so_review_sec_scheme_amt"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/tv_item_ratio_dis_amt"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:visibility="invisible"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/tv_item_free_qty_dis_amt"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:visibility="invisible"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />


                <TextView
                    android:id="@+id/tv_item_so_review_net_amt"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/tv_free_material_txt"
                    android:layout_width="@dimen/_400sdp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:gravity="left"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text" />

                <TextView
                    android:id="@+id/et_item_so_create_ord_qty"
                    android:layout_width="1dp"
                    android:layout_height="42dp"
                    android:gravity="right|center"
                    android:paddingRight="4dp"
                    android:textSize="@dimen/medium_text"
                    android:textColor="@color/BLACK"
                    style="@style/EditBookTextEdit"
                    android:inputType="numberDecimal" />



            </LinearLayout>


        </LinearLayout>


    </LinearLayout>
    <include layout="@layout/black_line_lay"/>


</LinearLayout>